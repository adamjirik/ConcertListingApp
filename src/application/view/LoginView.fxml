   <children>
      <PasswordField fx:id="pf_pass" layoutX="226.0" layoutY="200.0" />
      <TextField fx:id="tf_username" layoutX="226.0" layoutY="147.0" />
      <Label layoutX="111.0" layoutY="63.0" text="Welcome! Please login" textFill="WHITE">
         <font>
            <Font name="Courier New" size="30.0" />
         </font>
      </Label>
      <Button fx:id="btn_login" layoutX="274.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#loginClicked" text="LOGIN" />
   </children>
</AnchorPane>
